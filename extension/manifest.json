{
  "name": "#CheckYourSentiment",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "src/background.html"
  },
  "permissions": [
    "*://www.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": [
        "src/inject.js"
      ]
    }
  ]
}
